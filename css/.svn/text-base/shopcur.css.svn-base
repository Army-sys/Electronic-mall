@charset "";

#start div{position: relative;}
#start div .signds{position: absolute;left: 0;}
#start div>.sta{text-indent: 260px;}
#start div .signds .signs{position: absolute;left: 170px;top: 45px;font-size: 30px;font-weight: bold;color: #fff;line-height: 32px;}
#start form{margin: 10px 0;}
#start .start-bu{margin-bottom: 100px;}


/* 购物车 */
#shopcur .shopcurd .shopcurf{height: 60px;width: 100%;background: #ccc;}
#shopcur .shopcurd ul>li{float: left;line-height: 60px;}
#shopcur .shopcurd .shop1{   width: 260px;text-indent: 30px;}
#shopcur .shopcurd .shop2{ width: 312px;}
#shopcur .shopcurd .shop3{ width: 200px}
#shopcur .shopcurd .shop4{ width: 170px}
#shopcur .shopcurd .shop5{text-indent: 10px;}
#shopcur .shopcurd .shop6{ text-indent: 140px;}
#shopcur .shopcurd>.shopcur1 ul>li{float: left;line-height: 147px;}
#shopcur .shopcurd .inp{width: 20px;border: 1px solid #ccc;height: 20px;text-align: center;float: left;}
