#shopcur .shopcurd>.shopcur1 .shop4 {margin-top: 65px;}
#shopcur .shopcurd>.shopcur1 ul{border-bottom:1px solid #ccc;padding-bottom:10px;margin-bottom: 10px;}
#shopcur .shopcurd ul>li{float: left;line-height: 60px;}
#shopcur .shopcurd .shopcurl{height: 60px;width: 100%;border: 1px solid #ccc;}
#shopcur .shopcurd .shopcurl .shop1{   width: 100px;text-indent: 30px;}
#shopcur .shopcurd .shopcurl .shop2{   width: 670px;}
#shopcur .shopcurd .shopcurl .shop5{   width: 100px;}
#shopcur .shopcurd .shopcurl .shop6{width:160px;height: 60px;background: #f60;text-align: center;color: #fff;text-indent: 0;cursor: pointer;}



